<!--pages/index/orders_Incoming.wxml-->
<view id="app" class="app">
    <view class="orders-nav">
      <view class="orders-nav-view" bindtap='to_orders'>全部</view>
      <view class="orders-nav-view" bindtap='to_obligation'>待付款</view>
      <view class="orders-nav-view" bindtap='to_Shipped'>待发货</view>
      <view class="orders-nav-view">待收货<view class="line"></view></view>
      <view class="orders-nav-view" bindtap='to_received'>已收货</view>
    </view>
        <view class='orders_bg'></view>
    <!----------------------->
    <view class="orders-one orders-body">
      <image src="../assets/ordershop.png" alt=""  class="ordershop"/>
      <text class="shopname">店铺名称</text>
      <image src="../assets/order-right_03.png" alt="" class="orders-right"/>
      <image src="{{goods_image_1}}" alt="" class="orders-things"/>
      <view class="orders-things-name">{{goods_name_1}}</view>
      <text class="orders-money">￥{{goods_price_1}}</text>
    </view>
    <!----------------------->
</view>
